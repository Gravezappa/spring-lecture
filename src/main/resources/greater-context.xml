<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/>

    <bean class="com.spring.example5.InjectRandomIntAnnotationBeanPostProcessor"/>
    <bean class="com.spring.example5.SynchronizedAnnotationBeanPostProcessor"/>
    <bean class="com.spring.example5.PostSynchConstructInvokerApplicationListener"/>
    <bean id="greater" class="com.spring.example5.HiGreater"/>

</beans>